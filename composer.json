{
    "name": "algoritma/oro-dictionary-bundle",
    "description": "Dictionary Bundle for OroCommerce",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Algoritma srl",
            "email": "commerciale@algoritma.it",
            "homepage": "https://www.algoritma.it"
        }
    ],
    "license": "MIT",
    "require": {
        "oro/platform": "^4.2 || ^5.0 || ^5.1 || ^6.0 || ^6.1 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Algoritma\\Bundle\\DictionaryBundle\\": ""
        }
    },
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-main": "6.1-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true,
            "symfony/flex": true
        }
    }
}
