{
    "name": "algoritma/oro-dictionary-bundle",
    "description": "Dictionary Bundle for OroCommerce",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Algoritma srl",
            "email": "commerciale@algoritma.it",
            "homepage": "https://www.algoritma.it"
        }
    ],
    "license": "MIT",
    "require": {
        "oro/platform": "^4.2 || ^5.0 || ^5.1 || ^6.0 || ^6.1 || ^7.0"
    },
    "repositories": {
        "composer": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        }
    },
    "autoload": {
        "psr-4": {
            "Algoritma\\Bundle\\AlgoritmaDictionaryBundle\\": ""
        }
    },
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "6.1-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
